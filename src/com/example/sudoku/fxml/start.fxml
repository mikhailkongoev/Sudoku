    <stylesheets>
        <URL value="@/com/example/sudoku/style/style.css"/>
    </stylesheets>
    <Button fx:id="buttonStart" layoutX="150.0" layoutY="188.0" mnemonicParsing="false" onAction="#buttonStartOnAction"
            prefHeight="25.0" prefWidth="100.0" text="Играть!"/>
    <ChoiceBox fx:id="choiceBoxFieldSize" layoutX="436.0" layoutY="14.0" prefWidth="150.0" AnchorPane.rightAnchor="5.0"
